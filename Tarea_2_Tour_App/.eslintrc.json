{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugin": [
        "prettier"
    ],
    "rules": {
        "react/prop-types": 0
    },
    "parserOptions": {
        "ecmaVersion": 2016,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "indent": [
        "error",
        2,
        {
            "ignoredNodes": [
                "JSXElement *"
            ]
        }
    ]
}